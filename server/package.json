{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "get-beers": "ts-node ./scripts/download-beers.ts",
    "get-images": "ts-node ./scripts/download-images.ts",
    "add-info-to-beers": "ts-node ./scripts/add-extra-info-to-beers.ts",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/common": "^9.1.2",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/testing": "^9.0.0",
    "@prisma/client": "4.3.1",
    "@types/camelcase-keys": "^5.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@types/luxon": "^3.0.1",
    "@types/node": "^18.7.23",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "asyncctx": "^2.0.19",
    "axios": "^1.2.1",
    "camelcase-keys": "^8.0.2",
    "camelcase-object-deep": "^1.1.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs": "^0.0.1-security",
    "jest": "^28.1.2",
    "luxon": "^3.0.4",
    "nest-winston": "^1.7.0",
    "path": "^0.12.7",
    "prettier": "^2.7.1",
    "readline": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3",
    "winston": "^3.8.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
